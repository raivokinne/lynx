let println = fn(x) {
    if x == null { error "io.println: null value" }
    _print(x)
    return null
}

let writeStdout = fn(x) {
    if _write == null { error "io.writeStdout not available" }
    _write(x)
}

let readStdin = fn() {
    if _read == null { error "io.readStdin not available" }
    return _read()
}

let readLine = fn() {
    if _readLine == null { error "io.readLine not available" }
    return _readLine()
}
